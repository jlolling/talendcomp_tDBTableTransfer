<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="1.8" STATUS="FINAL"
		COMPATIBILITY="ALL" AUTHOR="cimt AG" RELEASE_DATE="20140813A"
		STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Databases/DB2</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="1">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CLOSE_CONNECTIONS" FIELD="CHECK"
			NUM_ROW="1" SHOW_IF="USE_EXISTING_CONNECTION == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION_SOURCE" FIELD="COMPONENT_LIST"
			FILTER="REGEXP:^(.)*(Connection)(.)*" NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true" />
			
		<PARAMETER NAME="CONNECTION_TARGET" FIELD="COMPONENT_LIST"
			FILTER="tDB2Connection" NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true" />

		<PARAMETER NAME="PROPERTY_SOURCE" FIELD="PROPERTY_TYPE" SHOW="true" 
			NUM_ROW="10" REPOSITORY_VALUE="DATABASE:IBM_DB2" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="SOURCE_HOST" FIELD="TEXT" NUM_ROW="12"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_PORT" FIELD="TEXT" SHOW="true"
			NUM_ROW="12" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"50002"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_DBNAME" FIELD="TEXT" NUM_ROW="13"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_SCHEMA_DB" FIELD="TEXT" NUM_ROW="13"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_USER" FIELD="TEXT" NUM_ROW="14"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_PASSWORD" FIELD="TEXT" NUM_ROW="14"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_QUERY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_QUERY" FIELD="MEMO_SQL" NUM_ROW="16"
			REQUIRED="true" SHOW_IF="USE_QUERY == 'true'" NB_LINES="5">
			<DEFAULT>"select * from schema.table where field=value...."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_TABLE" FIELD="TEXT" NUM_ROW="16"
			REQUIRED="true" SHOW_IF="USE_QUERY == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_WHERE" FIELD="TEXT" NUM_ROW="17"
			REQUIRED="false" SHOW_IF="USE_QUERY == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PROPERTY_TARGET" FIELD="PROPERTY_TYPE" SHOW="true" 
			NUM_ROW="20" REPOSITORY_VALUE="DATABASE:IBM_DB2" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />

		<PARAMETER NAME="TARGET_HOST" FIELD="TEXT" NUM_ROW="21"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_PORT" FIELD="TEXT" SHOW="true"
			NUM_ROW="22" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"50002"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_DBNAME" FIELD="TEXT" NUM_ROW="23"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_SCHEMA_DB" FIELD="TEXT" NUM_ROW="24"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_USER" FIELD="TEXT" NUM_ROW="25"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_PASSWORD" FIELD="TEXT" NUM_ROW="26"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TABLE" FIELD="TEXT" NUM_ROW="27"
			REQUIRED='true'>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_TRUNCATE" FIELD="CHECK" NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="EXCLUDE_COLUMNS" FIELD="TABLE"
			NUM_ROW="35" NB_LINES="10" >
			<ITEMS>
				<ITEM NAME="COLUMN" FIELD="TEXT" VALUE=""/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="LOG_INTERVAL" FIELD="TEXT" NUM_ROW="40">
			<DEFAULT>5</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="40">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="TEXT" NAME="SOURCE_FETCHSIZE" NUM_ROW="1"
			READONLY="false" REQUIRED="true" SHOW="true">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SOURCE_PROPERTIES" FIELD="TEXT" NUM_ROW="2"
			REQUIRED="false" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		</PARAMETER>

		<PARAMETER NAME="TARGET_BATCHSIZE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="3">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_PROPERTIES" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="4" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		</PARAMETER>

		<PARAMETER NAME="RUN_SYNCHRON" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SINGLE_INSTANCE" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_QUERY_SQL" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LOG_INSERT_SQL" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Log4J" MODULE="log4j-1.2.17.jar" REQUIRED="true" />
			<IMPORT NAME="SQLRunner" MODULE="sqlrunner-9.4.jar" REQUIRED="true" />
			<IMPORT NAME="cimt Table Transfer" MODULE="table_transfer-1.7.jar" REQUIRED="true" />
			<IMPORT NAME="db2Driver" MODULE="db2jcc4.jar" REQUIRED="true" />
			<IMPORT NAME="db2Driver_license" MODULE="db2jcc_license_cu.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="NB_INSERTS" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="AFTER" />
		<RETURN NAME="SOURCE_TABLE" TYPE="id_String" AVAILABILITY="AFTER" />
		<RETURN NAME="TARGET_TABLE" TYPE="id_String" AVAILABILITY="AFTER" />
	</RETURNS>

</COMPONENT>
